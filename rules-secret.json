{
  "rules": [
    {
      "pathname": "/robots.txt",
      "dest": "robots.alpha.spectrum.chat"
    },
    {
      "pathname": "/api",
      "dest": "api.secret.spectrum.chat"
    },
    {
      "pathname": "/api/**",
      "dest": "api.secret.spectrum.chat" 
    },
    {
      "pathname": "/auth",
      "dest": "api.secret.spectrum.chat" 
    },
    {
      "pathname": "/auth/**",
      "dest": "api.secret.spectrum.chat" 
    },
    {
      "pathname": "/websocket",
      "dest": "api.secret.spectrum.chat" 
    },
    {
      "pathname": "/websocket/**",
      "dest": "api.secret.spectrum.chat" 
    },
    {
      "dest": "hyperion.secret.spectrum.chat"
    }
  ]
}